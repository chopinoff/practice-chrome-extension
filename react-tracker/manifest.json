{
    "manifest_version": 3,
    "name": "React Tracker",
    "version": "1.0",
    "description": "Track your progress as you read through the official React documentation with ease.",
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "content_scripts": [
        {
            "js": ["scripts/content.js"],
            "matches": [
                "https://ko.react.dev/learn/*",
                "https://ko.react.dev/reference/*"
            ]
        }
    ]
}
